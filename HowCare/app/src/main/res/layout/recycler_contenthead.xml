<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="wrap_content"
  >

  <TextView
    android:id="@+id/contenthead_text"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="30dp"
    android:layout_marginStart="1dp"
    android:fontFamily="@font/notosanscjkkr_m"
    android:includeFontPadding="false"
    android:text="식사 전 준비"
    android:textColor="#ffa412"
    android:textSize="23.3sp"
    app:layout_constraintStart_toStartOf="@+id/contenthead_line"
    app:layout_constraintTop_toTopOf="parent"/>
  <ImageView
    android:id="@+id/contenthead_line"
    android:layout_width="0dp"
    android:layout_height="1dp"
    android:layout_marginTop="3dp"
    android:layout_marginStart="20dp"
    android:layout_marginEnd="20dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/contenthead_text"
    />
</androidx.constraintlayout.widget.ConstraintLayout>