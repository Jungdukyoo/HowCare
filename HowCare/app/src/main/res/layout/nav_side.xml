<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:layout_width="268dp"
  android:layout_height="match_parent"
  android:background="@mipmap/slide_bg"
  >
  <androidx.recyclerview.widget.RecyclerView
  android:id="@+id/navi_recyclerview1"
  android:layout_width="match_parent"
    android:layout_height="0dp"
    android:layout_marginBottom="31dp"
    app:layout_constraintBottom_toTopOf="@id/slide_favorite_cl"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    />
    <!--<androidx.recyclerview.widget.RecyclerView-->
      <!--android:id="@+id/navi_recyclerview1"-->
      <!--android:layout_width="match_parent"-->
      <!--android:layout_height="match_parent"-->

      <!--&gt;-->
  <!--</ScrollView>-->

  <androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/slide_favorite_cl"
    android:layout_width="268dp"
    android:layout_height="37dp"
    android:layout_marginBottom="1dp"
    android:background="@color/white"
    app:layout_constraintBottom_toTopOf="@id/slide_checklist_cl"
    app:layout_constraintStart_toStartOf="parent"
    >
    <ImageView
      android:id="@+id/navi_heart_image"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginLeft="17dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@mipmap/favorite_heart_empty"/>
    <TextView
      android:id="@+id/textView6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginLeft="10dp"
      android:fontFamily="@font/notosanscjkkr_m"
      android:text="저장된 항목 보기"
      android:textColor="#737373"
      android:textSize="13.5sp"
      android:includeFontPadding="false"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toEndOf="@id/navi_heart_image"
      app:layout_constraintTop_toTopOf="parent"/>
  </androidx.constraintlayout.widget.ConstraintLayout>

  <androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/slide_checklist_cl"
    android:layout_width="268dp"
    android:layout_height="37dp"
    android:layout_marginBottom="1dp"
    android:background="@color/white"
    app:layout_constraintBottom_toTopOf="@id/slide_reference_cl"
    app:layout_constraintStart_toStartOf="parent"
    >
    <ImageView
      android:id="@+id/navi_dot_image1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginLeft="17dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      android:src="@drawable/ic_baseline_check_24px"/>
    <TextView
      android:id="@+id/textView71"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginLeft="10dp"
      android:fontFamily="@font/notosanscjkkr_m"
      android:includeFontPadding="false"
      android:text="체크리스트"
      android:textColor="#737373"
      android:textSize="13.5sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toEndOf="@id/navi_dot_image1"
      app:layout_constraintTop_toTopOf="parent"/>
  </androidx.constraintlayout.widget.ConstraintLayout>
  <androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/slide_reference_cl"
    android:layout_width="268dp"
    android:layout_height="37dp"
    android:layout_marginBottom="16dp"
    android:background="@color/white"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    >
    <ImageView
      android:id="@+id/navi_dot_image"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginLeft="24dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@mipmap/ref_dot"/>
    <TextView
      android:id="@+id/textView7"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginLeft="@dimen/font_large16"
      android:fontFamily="@font/notosanscjkkr_m"
      android:includeFontPadding="false"
      android:text="참고문헌"
      android:textColor="#737373"
      android:textSize="13.5sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toEndOf="@id/navi_dot_image"
      app:layout_constraintTop_toTopOf="parent"/>
  </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>