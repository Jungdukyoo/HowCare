<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:id="@+id/serarch_result_linear"
  android:layout_width="match_parent"
  android:layout_height="wrap_content"
  android:orientation="vertical">
  <TextView
    android:id="@+id/search_result_title"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="21dp"
    android:layout_marginLeft="21dp"
    android:layout_marginRight="15dp"
    android:fontFamily="@font/batang_pro_b"
    android:includeFontPadding="false"
    android:singleLine="true"
    android:textColor="@color/greyish_brown_three"
    android:textSize="20sp"/>
  <TextView
    android:id="@+id/search_result_subtitle"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="12dp"
    android:layout_marginLeft="21dp"
    android:layout_marginRight="15dp"
    android:fontFamily="@font/notosanscjkkr_m"
    android:includeFontPadding="false"
    android:singleLine="true"
    android:textColor="@color/brown_grey"
    android:textSize="17sp"/>
  <TextView
    android:id="@+id/search_result_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="13dp"
    android:layout_marginBottom="18dp"
    android:layout_marginLeft="21dp"
    android:layout_marginRight="15dp"
    android:fontFamily="@font/batang_pro_b"
    android:includeFontPadding="false"
    android:lines="2"
    android:maxLines="2"
    android:textColor="@color/brownish_grey_two"
    android:textSize="14sp"/>
  <ImageView
    android:id="@+id/imageView17"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:scaleType="fitXY"
    app:srcCompat="@mipmap/favorite_line"/>
</LinearLayout>